        ArrayList<Double> amt = new ArrayList<Double>();
        boolean flag=true;
        System.out.println("Category");
        int l = sc.nextInt();
        String cat = arr[l];
        System.out.println("Enter the amount");
        while (flag)
        {
           in = sc.nextDouble();
            if(in == -1)
              flag = false;
              else
              amt.add(in);
        }

    }
}
